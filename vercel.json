{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/index.js" }],
  "files": [
    {
      "source": "/uploads/(.*)",
      "destination": "/uploads/$1"
    }
  ]
}
