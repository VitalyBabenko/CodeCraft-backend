{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "files": [
    {
      "source": "uploads/(.*)",
      "destination": "uploads/$1"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/index.js" }],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "status": 200
    }
  ]
}
